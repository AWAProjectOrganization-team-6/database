-- TOC entry 236 (class 1255 OID 16623)
-- Name: delete_order(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.delete_order() RETURNS trigger
    LANGUAGE plpgsql
    AS $$DECLARE
    old_order "order";
BEGIN
    IF (TG_TABLE_NAME = 'user') THEN
        IF (TG_OP = 'DELETE') THEN
            SELECT * INTO old_order FROM "order" WHERE "order".user_id = OLD.user_id;
            IF (old_order.restaurant_id IS NULL) THEN
                DELETE FROM "order" WHERE "order".order_id = old_order.order_id;
            END IF;
        END IF;
    ELSEIF (TG_TABLE_NAME = 'restaurant') THEN
        IF (TG_OP = 'DELETE') THEN
            SELECT * INTO old_order FROM "order" WHERE "order".restaurant_id = OLD.restaurant_id;
            IF (old_order.user_id IS NULL) THEN
                DELETE FROM "order" WHERE "order".order_id = old_order.order_id;
            END IF;
        END IF;
    END IF;
END;$$;


ALTER FUNCTION public.delete_order() OWNER TO postgres;

