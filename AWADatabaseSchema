CREATE FUNCTION public._test() RETURNS trigger
    LANGUAGE plpgsql
    AS $$BEGIN
    RAISE NOTICE '%: %', TG_OP, TG_TABLE_NAME;
    RAISE NOTICE 'OLD: %', OLD;
    RAISE NOTICE 'NEW: %', NEW;
    RETURN OLD;
END;$$;


